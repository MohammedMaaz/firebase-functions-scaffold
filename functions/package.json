{
  "name": "functions",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "serve": "npm run build && cross-env GOOGLE_APPLICATION_CREDENTIALS=./service_account.json firebase emulators:start --only functions",
    "shell": "npm run build && cross-env GOOGLE_APPLICATION_CREDENTIALS=./service_account.json firebase functions:shell",
    "start": "npm run shell",
    "deploy": "npm run build && firebase deploy --only functions",
    "logs": "firebase functions:log",
    "drop-collection": "firebase firestore:delete --recursive %npm_config_collection%"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/index.js",
  "dependencies": {
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "@google-cloud/tasks": "^2.0.0",
    "fs-extra": "^9.0.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "ngeohash": "^0.6.3",
    "node-fetch": "^2.6.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "promise-retry": "^1.1.1",
    "sharp": "^0.25.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "firebase-functions-test": "^0.2.0",
    "typescript": "^3.8.0"
  },
  "private": true
}
